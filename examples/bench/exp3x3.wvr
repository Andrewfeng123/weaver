(var x1 x2 x3 x4 x5 x6 n Int)

(assume (and (= x1 x2) (> x1 0)
             (= x3 x4) (> x3 0)
             (= x5 x6) (> x5 0)))

(par (while (< x1 n)
       (set! x1 (+ x1 x1)))
     (while (< x2 n)
       (set! x2 (+ x2 x2)))
     (while (< x3 n)
       (set! x3 (+ x3 x3)))
     (while (< x4 n)
       (set! x4 (+ x4 x4)))
     (while (< x5 n)
       (set! x5 (+ x5 x5)))
     (while (< x6 n)
       (set! x6 (+ x6 x6))))

(assume (not (and (= x1 x2) (= x3 x4) (= x5 x6))))
